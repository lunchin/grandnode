@model AffiliateModel

@(Html.LocalizedEditor<AffiliateModel, AffiliateLocalizedModel>("affiliate-seo-localized",
    @<div class="form-horizontal">
        <div class="form-body">
            <div class="form-group">
                <admin-label asp-for="@Model.Locales[item].MetaDescription" />
                <div class="col-md-9 col-sm-9">
                    <admin-textarea asp-for="@Model.Locales[item].MetaDescription"></admin-textarea>
                    <span asp-validation-for="@Model.Locales[item].MetaDescription"></span>
                </div>
            </div>
            <div class="form-group">
                <admin-label asp-for="@Model.Locales[item].MetaTitle" />
                <div class="col-md-9 col-sm-9">
                    <admin-input asp-for="@Model.Locales[item].MetaTitle" />
                    <span asp-validation-for="@Model.Locales[item].MetaTitle"></span>
                </div>
            </div>
            <div class="form-group">
                <admin-label asp-for="@Model.Locales[item].SeName" />
                <div class="col-md-9 col-sm-9">
                    <admin-input asp-for="@Model.Locales[item].SeName" />
                    <span asp-validation-for="@Model.Locales[item].SeName"></span>
                </div>
            </div>
            <input type="hidden" asp-for="@Model.Locales[item].LanguageId" />
        </div>
    </div>
    ,null))